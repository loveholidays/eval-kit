{
  "name": "@loveholidays/eval-kit",
  "version": "1.1.2",
  "description": "TypeScript SDK for content evaluation",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loveholidays/eval-kit.git"
  },
  "bugs": {
    "url": "https://github.com/loveholidays/eval-kit/issues"
  },
  "homepage": "https://github.com/loveholidays/eval-kit#readme",
  "keywords": [
    "evaluation",
    "content",
    "sdk",
    "typescript",
    "llm",
    "ai",
    "metrics",
    "bleu",
    "bert-score"
  ],
  "author": "",
  "license": "MIT",
  "sideEffects": false,
  "packageManager": "pnpm@10.20.0",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@changesets/cli": "^2.29.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "ai": "^4.3.16",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "fastest-levenshtein": "^1.0.16",
    "zod": "^3.23.8"
  }
}
